{
    "name": "brettt89/silverstripe-workflow",
    "description": "Adds configurable workflow support to the ORM",
    "keywords": ["silverstripe", "workflow"],
    "license": [
        "BSD-3-Clause"
    ],
    "authors": [
        {
            "name": "Brett Tasker",
            "email": "brett.tasker@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "silverstripe/framework": "^4.3",
        "symfony/workflow": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": { "SilverStripe\\Workflow\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "SilverStripe\\Workflow\\Tests\\": "tests/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
