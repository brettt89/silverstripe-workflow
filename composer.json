{
    "name": "silverstripe/silverstripe-workflow",
    "description": "Adds configurable workflow support to the ORM",
    "keywords": ["silverstripe", "workflow"],
    "authors": [
        {
            "name": "Brett Tasker",
            "email": "brett.tasker@gmail.com"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.3",
        "symfony/workflow": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "silverstripe/sqlite3": "^2.2",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\Workflow\\": "src/",
            "SilverStripe\\Workflow\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
